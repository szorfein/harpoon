#!/usr/bin/env ruby
# frozen_string_literal: true

require 'harpoon'

puts Harpoon.banner

files = [
  { name: 'wallpaper.zip',
    url: 'https://github.com/szorfein/walls/archive/main.zip',
  }
]

files.each { |file| Harpoon.download(file[:url], file[:name]) }

puts 'Harpoon v.' + Harpoon::VERSION
